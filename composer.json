{
	"name": "iteam-pro/garderobe",
	"description": "CakePHP UI plugin",
	"homepage": "http://iteam-pro.com",
	"keywords": ["cakephp", "ui"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "iTeam s.r.o.",
			"homepage": "https://iteam-pro.com"
		}
	],
	"support": {
		"issues": "https://github.com/iteam-pro/garderobe/issues",
		"source": "https://github.com/iteam-pro/garderobe"
	},
	"minimum-stability": "dev",
	"require": {
		"cakephp/cakephp": "~3.0",
		"cakephp/plugin-installer": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"Garderobe\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Garderobe\\Test\\": "tests"
		}
	}

}
