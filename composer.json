{
	"name": "mindforce/garderobe",
	"description": "CakePHP UI plugin",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/garderobe/issues",
		"source": "https://github.com/mindforce/garderobe"
	},
	"minimum-stability": "dev",
	"require": {
		"cakephp/cakephp": "~3.0",
		"cakephp/plugin-installer": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"Garderobe\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Garderobe\\Test\\": "tests"
		}
	}
}
