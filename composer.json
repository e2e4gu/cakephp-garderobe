{
    "name": "mindforce/cakephp-garderobe",
    "description": "Garderobe Core plugin for CakePHP",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/cakephp-garderobe/issues",
		"source": "https://github.com/mindforce/cakephp-garderobe"
	},
	"minimum-stability": "dev",
    "require": {
        "php": ">=5.4.16",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": ">=3.1",
        "mindforce/component-installer": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Garderobe\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garderobe\\Core\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
